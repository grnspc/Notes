{
	"name": "grnspc/notes",
	"description": "Provides the ability to add notes to your Eloquent models in Laravel.",
	"keywords": [
		"laravel",
		"notes",
		"noteable"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Nathan Robinson",
			"email": "grnspc.dev@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"laravel/framework": "^8.70 | ^9.0",
		"spatie/laravel-package-tools": "^1.11"
	},
	"require-dev": {
		"mockery/mockery": "^1.5",
		"orchestra/testbench": "^7.0",
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"Grnspc\\Notes\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Grnspc\\Notes\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit",
		"test:dox": "phpunit --testdox"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Grnspc\\Notes\\NotesServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
